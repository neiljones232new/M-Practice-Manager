{
  "name": "m-practice-manager",
  "version": "1.0.0",
  "description": "M Practice Manager - Modern client CRM for professional practices",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prestart": "node scripts/check-node-version.js",
    "dev": "NODE_ENV=development turbo run dev --parallel",
    "dev:api": "NODE_ENV=development pnpm -C apps/api start:dev",
    "dev:web": "NODE_ENV=development pnpm -C apps/web dev",
    "build": "NODE_ENV=production turbo run build",
    "build:api": "NODE_ENV=production pnpm -C apps/api build",
    "build:web": "NODE_ENV=production pnpm -C apps/web build",
    "start": "NODE_ENV=production turbo run start",
    "start:api": "NODE_ENV=production pnpm -C apps/api start:prod",
    "start:web": "NODE_ENV=production pnpm -C apps/web start",
    "test": "turbo run test",
    "test:api": "pnpm -C apps/api test",
    "test:web": "pnpm -C apps/web test",
    "db:generate": "pnpm -C apps/api exec prisma generate",
    "db:migrate": "pnpm -C apps/api exec prisma migrate dev --skip-generate",
    "db:deploy": "pnpm -C apps/api exec prisma migrate deploy",
    "backfill:practice": "pnpm ts-node -P tsconfig.scripts.json scripts/backfill-practice-config.ts",
    "backfill:practice:dry-run": "DRY_RUN=1 pnpm ts-node -P tsconfig.scripts.json scripts/backfill-practice-config.ts",
    "verify:startup": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"moduleResolution\":\"node\"}' ts-node scripts/verify-startup.ts",
    "db:up": "docker-compose up -d postgres",
    "db:down": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres",
    "data:import:clients": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"moduleResolution\":\"node\"}' ts-node scripts/import-clients-from-csv.ts",
    "migrate:compliance-links": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"moduleResolution\":\"node\"}' ts-node scripts/migrate-compliance-service-links.ts",
    "migrate:compliance-links:dry-run": "DRY_RUN=1 pnpm run migrate:compliance-links",
    "migrate:templates": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"moduleResolution\":\"node\"}' ts-node scripts/migrate-templates.ts",
    "migrate:templates:enhanced": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"moduleResolution\":\"node\"}' ts-node scripts/migrate-templates-enhanced.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "baseline-browser-mapping": "^2.9.19",
    "concurrently": "^8.2.2",
    "turbo": "^2.8.1",
    "typescript": "^5.3.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.28.2",
  "overrides": {
    "rxjs": "^7.8.1",
    "tmp": "^0.2.3",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23",
    "tar": "^7.5.3",
    "eslint": "^9.39.2"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "@swc/helpers": "0.5.18",
    "prisma": "^7.3.0",
    "project": "^0.1.6",
    "styled-jsx": "^5.1.7"
  }
}
